{
  "name": "storefront_backend",
  "version": "0.1.0",
  "description": "",
  "main": "server.ts",
  "scripts": {
    "start": "node ./dist/index.js",
    "watch": "nodemon",
    "watch:ts": "export NODE_ENV=dev && tsc-watch --esModuleInterop src/index.ts --outDir ./dist --onSuccess 'node ./dist/index.js'",
    "pretest": "npx db-migrate reset -e test && npx db-migrate up -c 3 -e test",
    "test": "export NODE_ENV=test PORT=3000 && jest  --notify --config jest.config.ts",
    "posttests": "npx db-migrate reset -e test",
    "test-jasmine": "export NODE_ENV=test PORT=5000 && ts-node node_modules/jasmine/bin/jasmine",
    "migrate:up": "npx db-migrate up -c 3",
    "migrate:down": "npx db-migrate down",
    "migrate:reset": "npx db-migrate reset",
    "seed:db": "db-migrate up 20220819011651-seeder-down",
    "tsc": "tsc",
    "build": "tsc"
  },
  "author": "Udacity",
  "license": "ISC",
  "dependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.9",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/morgan": "^1.9.2",
    "@types/pg": "^7.14.7",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.0",
    "cors": "^2.8.5",
    "db-migrate": "^0.11.12",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.10.0",
    "http-status-codes": "^2.1.4",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "pg": "^8.5.1",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@types/jest": "^28.1.6",
    "@types/supertest": "^2.0.12",
    "cross-env": "^7.0.3",
    "jasmine-spec-reporter": "^6.0.0",
    "jasmine-ts": "^0.3.0",
    "jest": "^28.1.3",
    "node-notifier": "^10.0.1",
    "nodemon": "^2.0.19",
    "supertest": "^6.2.2",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "tsc-watch": "^4.2.9"
  }
}
